{
  "name": "@adobe/target-decisioning-engine",
  "version": "1.8.0",
  "description": "Adobe Target JavaScript decisioning engine",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/*",
    "types/*",
    "src/**/!(*.spec).js",
    "test/test.utils.js",
    "test/schema/**/*.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/**/*.js test/**/*.js",
    "format": "prettier '**/*.js' '**/*.json' --write --config '../../.prettierrc.json' --ignore-path '../../.prettierignore'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "NODE_ENV=production rollup -c",
    "pretest": "npm run format && npm run lint",
    "test": "NODE_ENV=development jest",
    "prepublishOnly": "echo //${NPM_REGISTRY}/:_authToken=${NPM_AUTH_TOKEN} > .npmrc",
    "prepare": "rollup --config"
  },
  "dependencies": {
    "@adobe/target-tools": "^1.6.0",
    "json-logic-js": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.8.4",
    "@lwc/eslint-plugin-lwc": "^0.10.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fast-text-encoding": "^1.0.3",
    "http-status-codes": "^1.4.0",
    "https-proxy-agent": "^5.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "mockdate": "^2.0.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-visualizer": "^3.3.2"
  },
  "author": "",
  "license": "Apache-2.0"
}
