{
  "name": "@adobe/target-tools",
  "version": "1.6.2",
  "description": "",
  "main": "dist/index.js",
  "browser": "dist/index.browser.js",
  "module": "src/index.js",
  "files": [
    ".eslintrc.js",
    "babel.config.js",
    "jest.config.js",
    "delivery-api-client/**/*",
    "dist/*",
    "src/**/!(*.spec).js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "genPackageLock": "npm i --package-lock-only",
    "lint": "./node_modules/.bin/eslint src/**",
    "format": "prettier '**/*.js' '**/*.json' --write --config '../../.prettierrc.json' --ignore-path '../../.prettierignore'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "NODE_ENV=production rollup -c",
    "pretest": "npm run format && npm run lint",
    "test": "NODE_ENV=development jest",
    "prepublishOnly": "echo //${NPM_REGISTRY}/:_authToken=${NPM_AUTH_TOKEN} > .npmrc",
    "prepare": "rollup --config"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/target-nodejs-sdk.git",
    "directory": "packages/target-tools"
  },
  "dependencies": {
    "@adobe/reactor-object-assign": "^1.0.0",
    "parse-uri": "^1.0.0",
    "performance-now": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.8.4",
    "@lwc/eslint-plugin-lwc": "^0.10.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "core-js": "^3.6.5",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-status-codes": "^1.4.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-visualizer": "^3.3.2"
  },
  "author": "",
  "license": "Apache-2.0"
}
